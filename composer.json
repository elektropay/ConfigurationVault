{
    "name": "ucsdmath/configuration-vault",
    "type": "library",
    "description": "A specialized configuration handler used for confidential information and settings.",
    "keywords": ["configuration-vault", "ucsdmath", "configuration", "password", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2018-02-18",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "twig/twig": "2.4.4",
        "hashids/hashids": "2.0.4",
        "jenssegers/optimus": "0.2.3",
        "ucsdmath/filesystem": "2.5.x-dev",
        "ucsdmath/serialization": "2.5.x-dev"
    },
    "require-dev": {
        "symfony/debug": "4.0.4",
        "mockery/mockery": "1.0.0",
        "phpunit/phpunit": "7.0.2",
        "fzaninotto/faker": "1.7.1",
        "ucsdmath/date": "2.5.x-dev",
        "symfony/var-dumper": "4.0.4",
        "symfony/browser-kit": "4.0.4",
        "symfony/dom-crawler": "4.0.4",
        "symfony/options-resolver": "4.0.4",
        "squizlabs/php_codesniffer": "3.2.3"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Configuration\\ConfigurationVault\\": "src/ConfigurationVault/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
